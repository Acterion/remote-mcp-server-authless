		"drizzle-orm": "^0.44.4",
		"zod": "^3.25.67"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.1.3",
		"typescript": "5.8.3",
		"wrangler": "^4.27.0",
		"@modelcontextprotocol/inspector":"^0.16.5"
	}
